{
  "name": "@convergence-rfq/cli",
  "version": "4.0.24-rc.5",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/esm/index.mjs",
  "main": "dist/cjs/index.cjs",
  "bin": {
    "convergence": "dist/cjs/index.cjs"
  },
  "types": "dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "/dist/cjs",
    "/dist/esm",
    "/dist/types",
    "/src"
  ],
  "scripts": {
    "lint": "eslint --ext js,ts,tsx src",
    "lint:fix": "eslint --fix --ext js,ts,tsx src",
    "clean": "rimraf dist",
    "build": "pnpm clean && tsc && tsc-alias && rollup -c",
    "test:setup": "ts-mocha -p tsconfig.json --type-check --timeout 100000 tests/utils/setup.ts",
    "mocha": "ts-mocha -p tsconfig.json --type-check --timeout 100000 tests/**/*.spec.ts",
    "test": "yarn mocha --file tests/integration/setup.spec.ts tests/integration/bootstrap.spec.ts",
    "validator": "pkill solana-test-validator | echo '' && ts-node tests/validator/run.ts",
    "run": "ts-node src/index.ts"
  },
  "dependencies": {
    "@convergence-rfq/sdk": "4.0.24-rc.3",
    "@solana/web3.js": "^1.73.0",
    "commander": "^10.0.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1",
    "expect": "^29.5.0",
    "mocha": "^10.2.0",
    "sinon": "^15.0.1",
    "ts-mocha": "^10.0.0"
  }
}
