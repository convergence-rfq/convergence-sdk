{
  "name": "@convergence-rfq/cli",
  "description": "Official Convergence CLI",
  "version": "4.4.8",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/esm/index.mjs",
  "main": "dist/cjs/index.cjs",
  "types": "dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "/dist/cjs",
    "/dist/esm",
    "/dist/types",
    "/src"
  ],
  "keywords": [
    "convergence",
    "rfq",
    "solana",
    "blockchain",
    "cli"
  ],
  "author": "Convergence RFQ Maintainers <convergencerfq@tradersguild.so>",
  "homepage": "https://www.convergence.so",
  "repository": {
    "url": "https://github.com/convergence-rfq/convergence-sdk.git"
  },
  "bin": {
    "convergence": "dist/cjs/index.cjs"
  },
  "scripts": {
    "lint": "eslint --ext js,ts,tsx src",
    "lint:fix": "eslint --fix --ext js,ts,tsx src",
    "clean": "rimraf dist",
    "build": "yarn clean && tsc && tsc-alias && rollup -c",
    "test:init": "ts-mocha -p tsconfig.json --type-check --timeout 100000 tests/integration/init.ts",
    "test:coverage": "nyc ts-mocha --timeout 100000 tests/**/*.spec.ts",
    "test": "ts-mocha --timeout 100000 tests/**/*.spec.ts",
    "cli": "ts-node src/index.ts"
  },
  "dependencies": {
    "@convergence-rfq/sdk": "4.4.8",
    "@solana/web3.js": "^1.73.0",
    "@types/cookie": "^0.5.1",
    "commander": "^10.0.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1",
    "expect": "^29.5.0",
    "mocha": "^10.2.0",
    "sinon": "^15.0.1",
    "ts-mocha": "^10.0.0"
  }
}