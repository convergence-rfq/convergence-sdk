{
  "name": "@convergence-rfq/cli",
  "version": "4.0.23-rc.5",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/esm/index.mjs",
  "main": "dist/cjs/index.cjs",
  "bin": {
    "convergence": "dist/cjs/index.cjs"
  },
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "/dist/cjs",
    "/dist/esm",
    "/dist/types",
    "/src"
  ],
  "scripts": {
    "cli:run": "yarn build && node dist/cjs/index.cjs",
    "lint": "eslint --ext js,ts,tsx src",
    "lint:fix": "eslint --fix --ext js,ts,tsx src",
    "clean": "rimraf dist",
    "build": "pnpm clean && tsc && tsc-alias && rollup -c",
    "test": "ts-mocha -p tsconfig.json --type-check --parallel --timeout 10000 tests/**/*.spec.ts"
  },
  "dependencies": {
    "@convergence-rfq/sdk": "4.0.23-rc.6",
    "@solana/web3.js": "^1.73.0",
    "commander": "^10.0.0"
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1",
    "expect": "^29.5.0",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0"
  }
}
