{
    "extends": "../../../tsconfig.json",
    "include": ["./**/*"],
    "compilerOptions": {
      "target": "es2019",
      "module": "commonjs",
      "outDir": "../dist/test",
      "declarationDir": null,
      "declaration": false,
      "emitDeclarationOnly": false,
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
    }
  }