    "@convergence-rfq/risk-engine": "file:../../../convergence-program-library/risk-engine/js",
    "@convergence-rfq/spot-instrument": "file:../../../convergence-program-library/spot-instrument/js",
    "@rollup/plugin-json": "^4.1.0",
    "rollup": "^2.56.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@project-serum/anchor": "0.25.0",
    "@solana/spl-token": "^0.2.0"
  }
