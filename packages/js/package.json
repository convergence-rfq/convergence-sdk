{
  "name": "@convergence-rfq/sdk",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/cjs/index.cjs",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "/dist/cjs",
    "/dist/esm",
    "/dist/types",
    "/src"
  ],
  "scripts": {
    "build:docs": "yarn run typedoc src/index.ts",
    "type-check": "tsc --noEmit",
    "lint:test": "yarn eslint --ext js,ts,tsx src",
    "lint:fix": "yarn eslint --fix --ext js,ts,tsx src",
    "clean": "rimraf dist",
    "build": "yarn clean && tsc && tsc-alias && tsc -p test/tsconfig.json && tsc-alias -p test/tsconfig.json && rollup -c",
    "test": "tape dist/test/**/*.test.js && yarn test:exports",
    "test:exports": "node ./test/cjs-export.test.cjs && node ./test/esm-export.test.mjs",
    "preversion": "yarn build",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@convergence-rfq/psyoptions-european-instrument": "file:../../../convergence-program-library/psyoptions-european-instrument/js",
    "@convergence-rfq/rfq": "file:../../../convergence-program-library/rfq/js",
    "@convergence-rfq/risk-engine": "file:../../../convergence-program-library/risk-engine/js",
    "@convergence-rfq/spot-instrument": "file:../../../convergence-program-library/spot-instrument/js",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "rollup": "^2.56.3",
    "rollup-plugin-polyfill-node": "^0.10.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "spok": "^1.4.3",
    "tape": "^5.6.1",
    "typedoc": "^0.22.15",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@bundlr-network/client": "^0.8.9",
    "@metaplex-foundation/amman-client": "^0.2.2",
    "@metaplex-foundation/beet": "^0.7.1",
    "@metaplex-foundation/mpl-token-metadata": "^2.3.3",
    "@noble/ed25519": "^1.7.1",
    "@project-serum/anchor": "0.25.0",
    "@solana/spl-token": "^0.2.0",
    "@solana/web3.js": "^1.66.2",
    "@types/bn.js": "^5.1.1",
    "@types/debug": "^4.1.7",
    "@types/mime": "^3.0.1",
    "@types/tape": "^4.13.2",
    "bn.js": "5.2.1",
    "bs58": "^5.0.0",
    "debug": "^4.3.4",
    "eventemitter3": "^4.0.7",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "merkletreejs": "^0.3.0",
    "mime": "^3.0.0",
    "node-fetch": "^3.2.10",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.7.0"
  },
  "engines": {
    "node": ">=16.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "js"
  }
}
