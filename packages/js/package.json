{
  "name": "@convergence-rfq/sdk",
  "version": "4.0.24-rc.33",
  "sideEffects": false,
  "module": "dist/esm/index.mjs",
  "main": "dist/cjs/index.cjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "/dist",
    "/src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "description": "Official Convergence RFQ SDK",
  "keywords": [
    "rfq",
    "convergence",
    "solana",
    "blockchain"
  ],
  "author": "Convergence RFQ Maintainers <convergencerfq@tradersguild.so>",
  "homepage": "https://www.convergence.so",
  "repository": {
    "url": "https://github.com/convergence-rfq/convergence-sdk.git"
  },
  "scripts": {
    "lint": "eslint --ext js,ts,tsx src",
    "lint:fix": "eslint --fix --ext js,ts,tsx src",
    "clean": "rimraf dist",
    "build": "pnpm clean && tsc && tsc-alias && tsc -p tsconfig.json && tsc -p test/tsconfig.json && tsc-alias -p test/tsconfig.json && tsc-alias -p tsconfig.json && rollup -c",
    "mocha": "ts-mocha -p tests/tsconfig.json --timeout 100000 tests/**/*.spec.ts",
    "test": "yarn mocha",
    "test:tape": "tape dist/test/**/**/**/*.test.js && pnpm test:exports",
    "test:exports": "node ./test/cjs-export.test.cjs && node ./test/esm-export.test.mjs"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "3.0.0",
    "@bundlr-network/client": "^0.8.8",
    "@convergence-rfq/beet": "=0.7.10",
    "@convergence-rfq/beet-solana": "=0.4.11",
    "@convergence-rfq/psyoptions-american-instrument": "=2.2.2-rc.2",
    "@convergence-rfq/psyoptions-european-instrument": "=2.2.2-rc.2",
    "@convergence-rfq/rfq": "=2.2.2-rc.2",
    "@convergence-rfq/risk-engine": "=2.2.2-rc.2",
    "@convergence-rfq/spot-instrument": "=2.2.2-rc.2",
    "@coral-xyz/borsh": "^0.26.0",
    "@mithraic-labs/psy-american": "^0.2.3",
    "@mithraic-labs/tokenized-euros": "^0.2.3",
    "@noble/ed25519": "^1.7.1",
    "@noble/hashes": "^1.1.3",
    "@project-serum/anchor": "^0.25.0",
    "@solana/spl-token": "^0.3.5",
    "@solana/web3.js": "^1.63.1",
    "big.js": "^6.2.1",
    "bignumber.js": "^9.0.2",
    "black-scholes": "^1.1.0",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "buffer": "^6.0.3",
    "debug": "^4.3.4",
    "eventemitter3": "^4.0.7",
    "merkletreejs": "^0.2.32",
    "mime": "^3.0.0",
    "node-fetch": "^2.6.7"
  },
  "engines": {
    "node": ">=16.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "@convergence-rfq/sdk"
  },
  "devDependencies": {
    "@types/big.js": "^6.1.6",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1",
    "expect": "^29.5.0"
  }
}